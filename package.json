{
    "name": "vimdb",
    "version": "3.5.9",
    "description": "Yet another IMDb Scraper",
    "author": "Vinicius Egidio <me@vinicius.io>",
    "license": "ISC",
    "main": "dist/vimdb.js",
    "types": "dist/vimdb.d.ts",
    "targets": {
        "main": {
            "context": "node",
            "includeNodeModules": true
        }
    },
    "devDependencies": {
        "@parcel/packager-ts": "2.0.0-beta.2",
        "@parcel/transformer-typescript-tsc": "2.0.0-beta.2",
        "@parcel/transformer-typescript-types": "2.0.0-beta.2",
        "@types/cheerio": "0.22.28",
        "@types/html-entities": "1.2.16",
        "@types/jest": "26.0.22",
        "@types/node": "14.14.36",
        "@types/node-fetch": "2.5.8",
        "@typescript-eslint/eslint-plugin": "4.19.0",
        "@typescript-eslint/parser": "4.19.0",
        "await-to-js": "^3.0.0",
        "cheerio": "1.0.0-rc.5",
        "eslint": "7.22.0",
        "eslint-plugin-prettier": "^3.3.1",
        "file-type": "16.3.0",
        "html-entities": "2.1.1",
        "jest": "26.6.3",
        "node-fetch": "2.6.1",
        "parcel": "^2.0.0-beta.2",
        "prettier": "^2.2.1",
        "ts-jest": "26.5.4",
        "ts-loader": "8.0.18",
        "ts-node": "9.1.1",
        "typedoc": "^0.20.34",
        "typescript": "4.2.3"
    },
    "scripts": {
        "docs": "typedoc --options typedoc.json --disableSources",
        "build": "parcel build src/index.ts --no-source-maps --no-scope-hoist",
        "lint": "eslint . --fix --ext .ts",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vegidio/js-vimdb"
    },
    "keywords": [
        "imdb",
        "typescript",
        "scraper",
        "movies",
        "series"
    ]
}
